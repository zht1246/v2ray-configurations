{
	"inbound": {
		"port": 1080,
		"streamSettings": null,
		"settings": {
			"ip": "127.0.0.1",
			"udp": true,
			"clients": null,
			"auth": "noauth"
		},
		"protocol": "socks",
		"listen": "0.0.0.0"
	},
	"inboundDetour": [
		{
			"port": "1099",
			"listen": "0.0.0.0",
			"allocate": {
				"strategy": "always",
				"refresh": 5,
				"concurrency": 3
			},
			//用了这个socks协议打包后的文件就会直接在这里解析出域名，然后把域名发往vps的dns出ip，这样防止国内dns的污染
			"domainOverride": [
				"tls",
				"http"
			],
			"protocol": "dokodemo-door",
			"settings": {
				"network": "tcp,udp",
				"timeout": 30,
				"followRedirect": true
			}
		},
		{ //开放http端口
			"port": 10800,
			"streamSettings": null,
			"settings": {
				"ip": "127.0.0.1",
				"udp": true,
				"clients": null,
				"auth": "noauth"
			},
			"protocol": "http",
			"listen": "0.0.0.0"
		}
	],
	"outbound": {
		"streamSettings": {
			"network": "tcp",
			"kcpSettings": null,
			"wsSettings": null,
			"tcpSettings": null,
			"tlsSettings": {},
			"security": ""
		},
		"tag": "agentout",
		"protocol": "vmess",
		"mux": {
			"enabled": true
		},
		"settings": {
			"vnext": [
				{
					"users": [
						{
